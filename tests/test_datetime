"""Date and Time tests."""

import datetime
from unittest.mock import patch

from src.timedate import timedate


def test_date() -> None:
    """Test date function return current date."""
    assert timedate('Wie viel Uhr ist es') == f'Es ist {datetime.datetime.now().strftime("%H:%M")}'
